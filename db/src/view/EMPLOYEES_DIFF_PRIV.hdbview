VIEW EMPLOYEES_DIFF_PRIV AS
  SELECT ID, SITE, GENDER, AGE, SALARY, SALARY AS SALARY_DIFF_PRIV
  FROM EMPLOYEES
  WITH ANONYMIZATION (
    ALGORITHM 'DIFFERENTIAL_PRIVACY'
    PARAMETERS ''
    COLUMN ID PARAMETERS '{"is_sequence": true}'
    COLUMN SALARY_DIFF_PRIV PARAMETERS '{"is_sensitive": true, "epsilon": 0.5, "sensitivity": 5000}'
  )